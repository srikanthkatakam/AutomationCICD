<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="ErrorHandling" />
		</run>
	</groups>
	<listeners>
		<listener
			class-name="Ecommerce.SeleniumFrameworkDesign.TestComponents.Listeners" />
	</listeners>
	
	<test thread-count="5" name="Submit Order Test">
		<classes>
			<class
				name="Ecommerce.SeleniumFrameworkDesign.tests.SubmitOrderTest" />
		</classes>
	</test>

	<test thread-count="5" name="Error Validation Test">
		<classes>
			<class
				name="Ecommerce.SeleniumFrameworkDesign.tests.ErrorValidationTest" />
		</classes>
	</test>
</suite>
